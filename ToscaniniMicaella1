import re

file = open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1990.html')
           

pattern = 'year'
pattern1 = '\d{4}'
pattern2 = '<tr align="right"><td>'
pattern3 = '\d{1,}'
pattern4 = '[A-Z][a-z]*'

mi_diccionario = {}


for line in file:
    result = re.findall(pattern, line)
    if result:
        print(result)
        result = re.findall(pattern1, line)
        if result:
            print(result)

    result = re.findall(pattern2, line)
    if result:
        resultRank = re.findall(pattern3, line)
        print(resultRank)
        
        result = re.findall(pattern4, line)
        if result:
            print(result)
            mi_diccionario[str(resultRank)] = result
            
            print(result)
             
print(mi_diccionario)


mi_lista = []
file = open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1990.html')
for line in file:
    result = re.findall(pattern4, line)
    if result:
        result =re.search(pattern4, line)
        
        if result:
            #nuevos_datos = {resultRank: result}
            
            print(result)
            mi_lista.append(result.group(0))
mi_lista.sort()


mi_lista2 = []
file=open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1990.html')
for line in file:
    result =re.findall(pattern, line)
    if result:
        print(result)
        result =re.search(pattern1, line)
        if result:
            print(result.group(0))
            #mi_lista2.append(result.group(0))
            result =re.serch(pattern2, line)
            if result:
                print(result.group(0))
                resultRank =re.search(pattern3, line)
                print(resultRank.group(0))
                mi_lista.append(result.group(0))
                result =re.search(pattern4, line)
                if result:
                    print(re.split('s',result))
                    #nuevos_datos = {resultRank: result}
                    #mi_diccionario[str(resultRank)]= result
                    
                    mi_lista2.append((anio, resultRank.group(0),result.group(0)))
                    
print(mi_lista2.sort(key=lambda x: x[2], reverse=False))
print(mi_lista2)


import re

file = open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1992.html')
           

pattern = 'year'
pattern1 = '\d{4}'
pattern2 = '<tr align="right"><td>'
pattern3 = '\d{1,}'
pattern4 = '[A-Z][a-z]*'

mi_diccionario = {}


for line in file:
    result = re.findall(pattern, line)
    if result:
        print(result)
        result = re.findall(pattern1, line)
        if result:
            print(result)

    result = re.findall(pattern2, line)
    if result:
        resultRank = re.findall(pattern3, line)
        print(resultRank)
        
        result = re.findall(pattern4, line)
        if result:
            print(result)
            mi_diccionario[str(resultRank)] = result
            
            print(result)
             
print(mi_diccionario)


mi_lista = []
file = open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1992.html')
for line in file:
    result = re.findall(pattern4, line)
    if result:
        result =re.search(pattern4, line)
        
        if result:
            #nuevos_datos = {resultRank: result}
            
            print(result)
            mi_lista.append(result.group(0))
mi_lista.sort()


mi_lista2 = []
file=open('C:\\Users\\Micaella\\Desktop\\Micaella\\data\\baby1992.html')
for line in file:
    result =re.findall(pattern, line)
    if result:
        print(result)
        result =re.search(pattern1, line)
        if result:
            print(result.group(0))
            #mi_lista2.append(result.group(0))
            result =re.serch(pattern2, line)
            if result:
                print(result.group(0))
                resultRank =re.search(pattern3, line)
                print(resultRank.group(0))
                mi_lista.append(result.group(0))
                result =re.search(pattern4, line)
                if result:
                    print(re.split('s',result))
                    #nuevos_datos = {resultRank: result}
                    #mi_diccionario[str(resultRank)]= result
                    
                    mi_lista2.append((anio, resultRank.group(0),result.group(0)))
                    
print(mi_lista2.sort(key=lambda x: x[2], reverse=False))
print(mi_lista2)



